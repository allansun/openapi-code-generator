{
    "name": "allansun/openapi-code-generator",
    "description": "Code generator for OpenAPI or Swagger specification files",
    "license": "MIT",
    "keywords": [
        "swagger",
        "openapi",
        "open-api",
        "code-generator",
        "api",
        "client"
    ],
    "authors": [
        {
            "name": "Allan Sun",
            "email": "allan.sun@bricre.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "allansun/openapi-runtime": "3.*",
        "allansun/openapi-parser": "^2.1.2",
        "laminas/laminas-code": "^4",
        "mattketmo/camel": "^v1.1.0",
        "symfony/console": "^5.3.11",
        "symfony/filesystem": "^v5.3.4",
        "symfony/finder": "^v5.3.7",
        "symfony/monolog-bridge": "^4|^5.3.7",
        "symfony/options-resolver": "^5.3.7",
        "friendsofphp/php-cs-fixer": "^3.3.2",
        "psr/cache": "1.*",
        "symfony/yaml": "^v5.3.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "OpenAPI\\CodeGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenAPI\\CodeGenerator\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/openapi-code-generator"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
