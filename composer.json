{
    "name": "allansun/openapi-code-generator",
    "description": "Code generator for OpenAPI or Swagger specification files",
    "license": "MIT",
    "keywords": [
        "swagger",
        "openapi",
        "open-api",
        "code-generator",
        "api",
        "client"
    ],
    "authors": [
        {
            "name": "Allan Sun",
            "email": "allan.sun@bricre.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "allansun/openapi-runtime": "^3",
        "allansun/openapi-parser": "^2",
        "laminas/laminas-code": "^4.8.0",
        "friendsofphp/php-cs-fixer": "^2|^3",
        "mattketmo/camel": "^v1.1.0",
        "symfony/console": "^v5|^v6",
        "symfony/filesystem": "^v5|^v6",
        "symfony/finder": "^v5|^v6",
        "symfony/monolog-bridge": "^4|^v5|^v6",
        "symfony/options-resolver": "^v5|^v6",
        "symfony/yaml": "^v5|^v6",
        "psr/cache": "1.*|2.*|3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "OpenAPI\\CodeGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenAPI\\CodeGenerator\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/openapi-code-generator"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
