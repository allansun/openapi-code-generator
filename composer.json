{
  "name": "allansun/openapi-code-generator",
  "description": "Code generator for OpenAPI or Swagger specification files",
  "license": "MIT",
  "keywords": [
    "swagger",
    "openapi",
    "open-api",
    "code-generator",
    "api",
    "client"
  ],
  "authors": [
    {
      "name": "Allan Sun",
      "email": "allan.sun@bricre.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "allansun/openapi-runtime": "^2.0.1",
    "allansun/openapi-parser": "^2.1.2",
    "laminas/laminas-code": "^4.0.0",
    "mattketmo/camel": "^v1.1.0",
    "symfony/console": "^5",
    "symfony/filesystem": "^v5.2.4",
    "symfony/finder": "^v5.2.4",
    "symfony/monolog-bridge": "^4|^5",
    "symfony/options-resolver": "^5.2",
    "friendsofphp/php-cs-fixer": "^2.18"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload-dev": {
    "psr-4": {
      "OpenAPI\\CodeGenerator\\": "src",
      "OpenAPI\\CodeGenerator\\Tests\\": "tests"
    }
  },
  "bin": [
    "bin/openapi-code-generator"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
